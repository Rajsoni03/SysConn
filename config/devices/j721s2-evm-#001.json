{
    "uart_port_map": {
        "mcu0" : "/dev/ttyS0",
        "mcu1" : "/dev/ttyS1",
        "mcu2" : "/dev/ttyS2",
        "mcu3" : "/dev/ttyS3",
        "mpu0": "/dev/ttyS4",
        "mpu1": "/dev/ttyS5",
        "application" : "/dev/ttyS7",
        "sbl": "/dev/ttyS8",
        "debug": "/dev/ttyS6",
        "auto": null,
        "default": "/dev/ttyS0"
    },
    "boot_mode_map": {
        "uart": "000E",
        "emmc": "000D",
        "sd_card": "000C",
        "usb": "000B",
        "network": "000A",
        "default": "000E"
    },
    "health_check": {
        "uart_ports": ["mcu0", "mcu1", "mcu2", "mcu3", "mpu0", "mpu1", "application", "sbl", "debug"],
        "check_interval_seconds": 60,
        "error_threshold": 5
    },
    "power_config": {
        "module": "relay",
        "settings": {
            "ip_address": "192.168.1.100",
            "port": 1234,
            "username": "admin",
            "password": "password",
            "outlet_number": 2
        }
    }
}